<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.runtime.remoting>
		<application name="OperationalUI2">
			<service>
				<wellknown mode="Singleton"
                   type="UrbanChallenge.NameService.ObjectDirectory,NameService"
                   objectUri="tcp://chinook:12345/ObjectDirectory"   />
			</service>
			<channels>
				<channel ref="tcp" port="0" timeout="5000">
					<clientProviders>
						<formatter ref="binary" />
					</clientProviders>
					<serverProviders>
						<formatter ref="binary" typeFilterLevel="Full" />
					</serverProviders>
				</channel>
			</channels>
		</application>
		<customErrors mode="Off"/>
	</system.runtime.remoting>
	<datasetConfig>
		<!--<clientConfig port="30079" useMulticast="true" multicastAddress="224.0.2.99"/>-->
	</datasetConfig>
</configuration>

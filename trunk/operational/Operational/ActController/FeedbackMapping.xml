      <enum>
        <value in="0" outenum="Unknown"/>
        <value in="1" outenum="EStop"/>
        <value in="2" outenum="Pause"/>
        <value in="3" outenum="Run"/>
        <value in="4" outenum="TransitioningFromPause"/>
        <value in="5" outenum="Human"/>
        <value in="default" outenum="Unknown"/>

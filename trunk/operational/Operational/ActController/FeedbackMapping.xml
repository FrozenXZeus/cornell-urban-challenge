        <value in="2" outint="2"/>
        <value in="3" outint="3"/>
        <value in="default" outint="-1"/>
      </intMap>
    </field>
    <field name="actual pedal position" intype="u8" outtype="int" dsitem="actual pedal position">
      <conversion>
        <scale>0.003921568627450980392156862745098</scale>
      </conversion>
    </field>
